{
  "name": "Hass Telegram bot",
  "version": "1",
  "slug": "hass_telegram_bot",
  "description": "Bot with shopping list",
  "arch": [
    "armhf",
    "armv7",
    "aarch64",
    "amd64",
    "i386"
  ],
  "startup": "application",
  "boot": "auto",
  "host_network": true,
  "options": {
    "port": 8574,
    "telegram": {
      "login": "",
      "token": "",
      "users": [
        {
          "username": "",
          "chatId": ""
        }
      ]
    }
  },
  "schema": {
    "port": "int",
    "telegram": {
      "users": ["match(.+)"],
      "login": "str",
      "token": "str"
    }
  }
}